<view class="container">
  <view class="header">
    <view class="back-btn" bindtap="goBack">
      <text class="back-icon">←</text>
    </view>
    <view class="title">历史记录</view>
  </view>
  <view wx:for="{{history}}" wx:key="index" class="history-item">
    <view class="message">{{item}}</view>
  </view>
</view>
